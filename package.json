{
  "name": "icrs-sparc",
  "version": "1.0.0",
  "description": "ICRS SPARC - Foreign Trade Zone Operations Management System",
  "main": "src/backend/api/index.js",
  "scripts": {
    "start": "node src/backend/api/index.js",
    "dev": "nodemon src/backend/api/index.js",
    "build": "echo \"No build step needed for backend\" && exit 0",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/backend --ext .js",
    "lint:fix": "eslint src/backend --ext .js --fix",
    "frontend:install": "cd src/frontend && npm install",
    "frontend:start": "cd src/frontend && npm start",
    "frontend:build": "cd src/frontend && npm run build",
    "frontend:test": "cd src/frontend && npm test",
    "frontend:lint": "cd src/frontend && npm run lint",
    "dev:full": "concurrently \"npm run dev\" \"npm run frontend:start\"",
    "build:full": "npm run build && npm run frontend:build",
    "install:all": "npm install && npm run frontend:install"
  },
  "keywords": [
    "foreign-trade-zone",
    "inventory-management",
    "customs",
    "warehouse-management",
    "express-api",
    "supabase"
  ],
  "author": "Integrator Consulting, LLC",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@sentry/react": "^10.7.0",
    "@supabase/supabase-js": "^2.45.4",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.4.0",
    "helmet": "^7.1.0",
    "playwright": "^1.55.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "concurrently": "^8.2.2",
    "eslint": "^9.9.1",
    "jest": "^29.7.0",
    "nodemon": "^3.1.4",
    "puppeteer": "^24.17.1",
    "supabase": "^2.39.2",
    "supertest": "^7.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/integrator-consulting/icrs-sparc"
  }
}
