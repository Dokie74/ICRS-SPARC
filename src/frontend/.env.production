# Production environment variables for ICRS SPARC Frontend
# These will be used when deployed to Vercel

# API Configuration - will use same domain as frontend in production
REACT_APP_API_URL=/api

# Build Configuration
GENERATE_SOURCEMAP=false

# Vercel Analytics
REACT_APP_VERCEL_ANALYTICS_ID=

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_ERROR_REPORTING=true

# Cache Configuration  
REACT_APP_CACHE_DURATION=300000